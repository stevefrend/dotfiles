{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "hide_root_location": true,
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue5ff {{ .Path }}",
          "foreground": "#3EC669",
          "background": "#29315A",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ .UserName }}",
          "foreground": "#E64747",
          "background": "#29315A",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .HEAD }}",
          "foreground": "#43CCEA",
          "background": "#29315A",
          "type": "git",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#E4F34A",
          "background": "#29315A",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#7FD5EA",
          "background": "#29315A",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#42E66C",
          "background": "#29315A",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#E64747",
          "background": "#29315A",
          "type": "java",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 150
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ .FormattedMs }}",
          "foreground": "#AEA4BF",
          "background": "#29315A",
          "type": "executiontime",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "‚ùØ",
          "foreground": "#7FD5EA",
          "background": "#29315A",
          "type": "text",
          "style": "diamond"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
